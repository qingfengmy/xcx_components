/* pages/home/index.wxss */
.container{
  background: #f4f4f4;
  display: flex;
  flex-direction: column;
  height: 100vh;
  padding: 0;
}
.item{
  background: white;
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  justify-content: center;
}

.item:not(:last-child)::after {
  content: '';
  background: #f4f4f4;
  height: 2rpx;
  display: block;

}